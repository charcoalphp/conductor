{
    "admin": {
        "lists": {
            "app.default": {
                "properties": [
                    "title",
                    "last_modified",
                    "active"
                ],
                "orders": [
                    {
                        "property": "position",
                        "mode": "asc"
                    }
                ],
                "list_actions": [
                    {
                        "ident": "create",
                        "url": "object/edit?obj_type={{ objType }}",
                        "permissions": [ "object/create" ]
                    }
                ],
                "properties_options": {
                    "title": {
                        "display_type": "charcoal/admin/property/display/hierarchical"
                    },
                    "template_ident": {
                        "view_options": {
                            "attr": {
                                "width": "15%"
                            }
                        }
                    },
                    "last_modified": {
                        "view_options": {
                            "attr": {
                                "width": "15%"
                            }
                        }
                    },
                    "active": {
                        "view_options": {
                            "attr": {
                                "width": "5%",
                                "class": "text-center"
                            }
                        }
                    }
                }
            }
        },
        "default_list": "app.default",
        "forms": {
            "app.create": {
                "groups": {
                    "cms.info": {
                        "priority": 100
                    }
                },
                "sidebars": {
                    "default": {}
                }
            },
            "app.edit": {
                "groups": {
                    "cms.info": {
                        "priority": 100
                    },
                    "cms.advanced": {
                        "priority": 200
                    },
                    "cms.dev": {
                        "priority": 300
                    },
                    "cms.meta": {
                        "priority": 400
                    },
                    "cms.table": {
                        "priority": 500
                    },
                    "cms.revisions": {
                        "priority": 600
                    }
                },
                "sidebars": {
                    "default": {}
                }
            }
        },
        "form_sidebars": {
            "default": {
                "properties": [
                    "created",
                    "created_by",
                    "last_modified",
                    "last_modified_by"
                ]
            }
        },
        "dashboards": {
            "admin.edit": {
                "layout": {
                    "structure": [
                        { "columns": [ 1 ] }
                    ]
                },
                "widgets": {
                    "form": {
                        "type": "charcoal/admin/widget/object-form",
                        "form_ident": "{{ formIdent }}",
                        "group_display_mode": "tab"
                    }
                }
            },
            "admin.table": {
                "layout": {
                    "structure": [
                        { "columns": [ 1 ] }
                    ]
                },
                "widgets": {
                    "table": {
                        "type": "charcoal/admin/widget/hierarchical-table",
                        "obj_type": "{NAMESPACE}/{OBJECT_TYPE}",
                        "collection_ident": "app.default",
                        "sortable": true
                    }
                }
            }
        },
        "default_create_dashboard": "admin.edit",
        "default_edit_dashboard": "admin.edit",
        "default_collection_dashboard": "admin.table"
    }
}
